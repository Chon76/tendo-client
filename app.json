{
  "expo": {
    "name": "tendo",
    "owner": "chon76",
    "slug": "tendo",
    "version": "pre-alpha",
    "orientation": "portrait",
    "icon": "./src/common/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/common/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/common/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.chon76.tendo"
    },
    "web": {
      "favicon": "./src/common/assets/images/favicon.png"
    }
  }
}
